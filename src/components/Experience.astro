---
import ExperienceItem from "./ExperienceItem.astro";
const { experience } = Astro.props;
---

<ol class="relative border-s border-gray-400 dark:border-gray-200 ml-3">
  {
    experience.map((exp) => (
      <li class="mb-10 ms-4">
        <ExperienceItem {...exp} />
      </li>
    ))
  }
</ol>
